<Window x:Class="jlq_MBE_BattleSimulation.Dialogs.Dialog_Settings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:jlq_MBE_BattleSimulation.Dialogs"
        mc:Ignorable="d"
        Title="设置" Height="300" Width="300">
    <Grid>
        <Grid HorizontalAlignment="Stretch" Height="70" VerticalAlignment="Bottom">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Button x:Name="buttonSave" Content="保存" HorizontalAlignment="Center" VerticalAlignment="Center" Width="75" Click="buttonSave_Click"/>
            <Button x:Name="buttonCancel" Content="取消" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" Width="75" Click="buttonCancel_Click"/>
        </Grid>

    </Grid>
</Window>
